<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ruru.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ClockActivity" />
        <activity android:name=".activity.CircleActivity" />
        <activity android:name=".activity.ProgressActivity" />
        <activity android:name=".activity.TextMoveActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".activity.PullRefreshLayoutActivity" />
        <activity android:name=".activity.HorizontalSVActivity" />
        <activity android:name=".activity.WaveActivity"></activity>
    </application>

</manifest>